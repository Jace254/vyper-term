{
  "name": "vyper-term",
  "type": "module",
  "version": "1.0.19",
  "description": "Web SSH Utility",
  "scripts": {
    "dev": "concurrently \"npm run server --prefix server\" \"vite\"",
    "build": "vue-tsc && vite build --mode production",
    "build:release": "vue-tsc && vite build --mode production --config release.config.ts",
    "preview": "vite preview",
    "lint": "eslint .",
    "format": "eslint --fix .",
    "release": "pnpm build:release && npx bumpp && npm publish"
  },
  "dependencies": {
    "@unocss/reset": "^0.58.2",
    "@vueuse/core": "^10.7.1",
    "lucide-vue-next": "^0.303.0",
    "radix-vue": "^1.2.6",
    "socket.io-client": "^4.5.1",
    "theme-vitesse": "^0.7.5",
    "utf8": "^3.0.0",
    "vue": "^3.3.11",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.2",
    "@iconify/json": "^2.2.166",
    "@tanstack/vue-table": "^8.11.2",
    "@types/node": "^20.10.6",
    "@unocss/preset-mini": "^0.58.2",
    "@vitejs/plugin-vue": "^4.5.2",
    "bumpp": "^9.2.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "eslint": "npm:eslint-ts-patch@^8.56.0-0",
    "tailwind-merge": "^2.2.0",
    "typescript": "^5.2.2",
    "unocss": "^0.58.2",
    "vite": "^5.0.8",
    "vite-plugin-css-injected-by-js": "^3.3.1",
    "vite-plugin-dts": "^3.7.0",
    "vue-tsc": "^1.8.25"
  }
}
